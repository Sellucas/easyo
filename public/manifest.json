{
  "manifest_version": 3,
  "version": "1.0",
  "name": "easyo - SEO Analyzer",
  "description": "A Chrome extension to analyze SEO data.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting", "tabs", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"],
  "icons": {
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
